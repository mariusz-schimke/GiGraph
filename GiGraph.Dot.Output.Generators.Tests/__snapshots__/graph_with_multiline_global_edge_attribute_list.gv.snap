// graph
digraph
{
    /* graph attributes */

    // comment
    comment = comment
    // label
    label = "Foo Graph"

    // global node attributes
    node [ distortion = 2, shape = rectangle ]
    // global edge attributes
    edge
    [
        arrowhead = curve
    ]

    /* nodes */

    // node comment
    foo [ /* node attributes */  /* label */ label = foo ]
    // node group comment
    foo, bar, baz [ /* node group attributes */  /* label */ label = foo ]

    /* edges */

    // edge comment
    /* tail */ foo -> /* head */ bar [ /* edge attributes */  /* color */ color = red ]
    // edge sequence comment
    /* endpoint 1 */ foo -> /* endpoint 2 */ bar -> /* endpoint 3 */ baz [ /* edge sequence attributes */  /* color */ color = red ]

    /* subsection 1 */

    /* subgraphs */

    // subgraph
    {
    }

    /* clusters */

    // cluster
    subgraph "cluster cluster 1"
    {
    }
}
